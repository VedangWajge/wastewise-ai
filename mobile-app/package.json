{
  "name": "wastewise-mobile",
  "version": "1.0.0",
  "description": "WasteWise Mobile App - AI-Powered Smart Waste Segregation",
  "main": "index.js",
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "xcodebuild -workspace ios/WasteWise.xcworkspace -scheme WasteWise -configuration Release -destination generic/platform=iOS -archivePath WasteWise.xcarchive archive"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-vector-icons": "^10.0.2",
    "react-native-image-picker": "^7.0.3",
    "react-native-permissions": "^3.10.1",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-push-notification": "^8.1.1",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-reanimated": "^3.6.2",
    "react-native-safe-area-context": "^4.7.4",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^13.14.0",
    "axios": "^1.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-snap-carousel": "^3.9.1",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "react-native-linear-gradient": "^2.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/runtime": "^7.22.9",
    "@react-native/eslint-config": "^0.72.2",
    "metro-react-native-babel-preset": "^0.76.8",
    "@types/react": "^18.2.7",
    "@types/react-test-renderer": "^18.2.0",
    "babel-jest": "^29.6.2",
    "eslint": "^8.45.0",
    "jest": "^29.6.2",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.2.2",
    "prettier": "^3.1.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "node": ">=16"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VedangWajge/wastewise-ai.git"
  },
  "keywords": [
    "react-native",
    "mobile",
    "waste-management",
    "ai",
    "classification",
    "recycling",
    "environment",
    "sustainability"
  ],
  "author": "Vedang Wajge",
  "license": "MIT",
  "private": true
}
