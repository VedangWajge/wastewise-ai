				{
					"name": "‚≠ê Classify Waste Image",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Classification successful\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.success).to.be.true;",
									"    pm.expect(jsonData.waste_type).to.exist;",
									"    pm.expect(jsonData.confidence).to.be.a('number');",
									"    pm.expect(jsonData.provider_used).to.exist;",
									"});",
									"",
									"pm.test(\"Recommendations provided\", function () {",
									"    var jsonData = pm.response.json();",
									"    pm.expect(jsonData.recommendations).to.be.an('array');",
									"    pm.expect(jsonData.recommendations.length).to.be.above(0);",
									"});"
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"method": "POST",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{access_token}}",
								"type": "text",
								"description": "Optional - adds user rewards if logged in",
								"disabled": false
							}
						],
						"body": {
							"mode": "formdata",
							"formdata": [
								{
									"key": "image",
									"type": "file",
									"src": [],
